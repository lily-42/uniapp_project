<u-modal vue-id="4b74a4e2-1" show="{{ratingShow}}" title="给小程序打个分吧" confirmText="提交评价" closeOnClickOverlay="{{true}}" data-event-opts="{{[['^confirm',[['confirm']]],['^close',[['closeRating']]]]}}" bind:confirm="__e" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="content-box"><view class="star-box"><block wx:for="{{starList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectStar',['$0',index],[[['starList','',index]]]]]]]}}" class="star" bindtap="__e"><view class="{{['_i','iconfont','pig--wujiaoxing',[(curentStarIndex!==''&&index<=curentStarIndex)?'active':'']]}}"></view><text class="{{['title',[(curentStarIndex===index)?'select':'']]}}">{{item.title}}</text></view></block></view><view class="tags-box"><block wx:for="{{tagsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectTag',['$0',index],[[['tagsList','',index]]]]]]]}}" class="{{['tags',[(index===curentTagIndex)?'active':'']]}}" bindtap="__e">{{''+item.tag+''}}</view></block></view><view class="spit-box"><text class="spit">吐槽：</text><u--textarea bind:input="__e" vue-id="{{('4b74a4e2-2')+','+('4b74a4e2-1')}}" placeholder="请输入内容" count="{{true}}" value="{{spitValue}}" data-event-opts="{{[['^input',[['__set_model',['','spitValue','$event',[]]]]]]}}" bind:__l="__l"></u--textarea></view><view class="see-all-rating"><view data-event-opts="{{[['tap',[['toAllrating',['$event']]]]]}}" class="see-text" bindtap="__e">查看所有评价</view></view></view></u-modal>